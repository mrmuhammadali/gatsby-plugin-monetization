{
    "name": "gatsby-plugin-monetization",
    "version": "1.0.0-rc.5",
    "description": "Helpers and hooks to speed up your integration with Web Monetization API",
    "author": "Muhammad Ali <hi@muhammadali.io>",
    "license": "MIT",
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "react",
        "monetize",
        "monetization",
        "web-monetization"
    ],
    "homepage": "https://github.com/mrmuhammadali/gatsby-plugin-monetization/tree/master#readme",
    "repository": "https://github.com/mrmuhammadali/gatsby-plugin-monetization",
    "main": "index.js",
    "private": false,
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "prepublishOnly": "yarn lint",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix"
    },
    "dependencies": {
        "react-monetize": "^1.0.0-rc.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "babel-eslint": "^10.1.0",
        "babel-preset-gatsby-package": "^0.4.3",
        "cross-env": "^7.0.2",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-plugin-flowtype": "^5.1.3",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "gatsby": "^2.23.0",
        "husky": ">=4",
        "lint-staged": ">=10",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "peerDependencies": {
        "gatsby": ">=2.x",
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "engines": {
        "node": ">=10.12.0"
    }
}
